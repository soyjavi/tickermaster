{
  "name": "tickermaster",
  "description": "A new expanded and private ticker running on the blockchain.",
  "version": "0.6.0",
  "dependencies": {
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cron": "1.7.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/node": "7.7.4",
    "@babel/plugin-proposal-throw-expressions": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "eslint": "6.7.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "nodemon": "2.0.1",
    "parcel-bundler": "1.12.4"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "parcel build src/scripts/index.js src/styles/index.css",
    "watch": "parcel src/scripts/index.js src/styles/index.css",
    "server": "babel-node src/index.js",
    "start": "yarn build && yarn server",
    "start:dev": "nodemon --ignore '/dist/**' --ignore '/store/**' --exec yarn start",
    "test": "jest --detectOpenHandles",
    "lint": "eslint src/**"
  },
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": false
}
