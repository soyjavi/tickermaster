{
  "name": "masterticker",
  "description": "A new expanded and private ticker running on the blockchain.",
  "version": "0.2.5",
  "dependencies": {
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cron": "^1.7.2",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "2.18.2",
    "jest": "23.6.0",
    "nodemon": "1.19.1"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**",
    "test": "jest --detectOpenHandles",
    "build": "babel -d ./dist ./src -s",
    "start": "npm run build && NODE_ENV=production node ./dist/index.js",
    "start:dev": "nodemon --ignore '/dist/**' --ignore '/store/**' --exec NODE_ENV=development babel-node src/index.js"
  },
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": false
}
